        "from sklearn.preprocessing import LabelEncoder\n",
        "\n",
        "label_encode = [col for col in df.columns if df[col].dtype == 'object' and df[col].nunique() == 2]\n",
        "\n",
        "le = LabelEncoder()\n",
        "\n",
        "for col in label_encode:\n",
        "  df[col]= le.fit_transform(df[col])"
